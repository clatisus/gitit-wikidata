#include <bits/stdc++.h>

using ll = long long;
const int N = 100;

int digit[N], digit1[N];

void solve(){
    ll x, y;
    scanf("%lld%lld", &x, &y);
    if (y == 0){
        puts("Yes");
        return;
    }
    while (y % 2 == 0){
        y /= 2;
    }
    for (int i = 0; i < 32; ++ i){
        digit[i] = x % 2;
        x /= 2;
    }
    int dcnt = 0;
    while (y){
        digit1[dcnt ++] = y % 2;
        y /= 2;
    }
    for (int i = 0; i + dcnt - 1 < 32; ++ i){
        bool flag = true;
        for (int j = 0; j < dcnt; ++ j){
            if (digit[i + j] != digit1[j]){
                flag = false;
                break;
            }
        }
        if (flag){
            puts("Yes");
            return;
        }
    }
    puts("No");
}

int main(){
    int test;
    scanf("%d", &test);
    while (test --){
        solve();
    }
    return 0;
}
