#include <bits/stdc++.h>

const int max_N = (int) 1e5 + 21;

int n, m, next[max_N][26][2];

char s[max_N], t[max_N];

bool check(int st) {
    for (int i = 1; i <= m; ++i) {
        int c = s[i] - 'a';
        if (next[st][c][st & 1] > n) {
            return false;
        }
        st = next[st][c][st & 1] + 1;
    }
    return true;
}

int main() {
    scanf("%s%s", s + 1, t + 1);
    n = strlen(t + 1);
    m = strlen(s + 1);
    if (m > n || (n - m) % 2 == 1) {
        puts("NO");
        return 0;
    }
    for (int c = 0; c < 26; ++c) {
        next[n + 1][c][0] = next[n + 1][c][1] = n + 1;
    }
    for (int i = n; i; --i) {
        memcpy(next[i], next[i + 1], sizeof next[i + 1]);
        next[i][t[i] - 'a'][i & 1] = i;
    }
    int c = s[1] - 'a';
    puts(check(next[1][c][1]) ? "YES" : "NO");
}
