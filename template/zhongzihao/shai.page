// 线性筛模板 

#include<bits/stdc++.h>

const int N = 1000010;

int min[N], mu[N];
std::vector <int> prime;

int main(){
	mu[1] = 1;
	for(int i = 2; i < N; ++ i){
		if(!min[i]){
			prime.push_back(i);
			mu[i] = -1;
			min[i] = i;
		}
		for(int j = 0, k, sz = (int) prime.size(); j < sz && i * prime[j] < N; ++ j){
			min[k = i * prime[j]] = prime[j];
			if(i % prime[j] == 0){
				mu[k] = 0;
				break;
			}
			mu[k] = mu[i] * -1;
		}
	}
	return 0;
}
